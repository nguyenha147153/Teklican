                            <ul>
                                <li><a href="ip12pm.html">Iphone 12</a></li>
                                <li><a href="">Iphone 11</a></li>
                                <li><a href="">Iphone SE</a></li>
                                <li><a href="">Iphone X</a></li>
                                <li><a href="">Iphone 8</a></li>
                                <li><a href="">Iphone 7</a></li>
                            </ul>
                        </div>
                    </li>
                    <li>
                        <a><i class="fa fa-mobile"></i></a>@Html.ActionLink("Samsung", "Samsung", "Home")
                        <div class="sub-menu-1">
                            <ul>
                                <li><a href="">Galaxy Fold</a></li>
                                <li><a href="">Galaxy Z Flip</a></li>
                                <li><a href="">Galaxy Note</a></li>
                                <li><a href="">Galaxy S</a></li>
                                <li><a href="">Galaxy A</a></li>
                            </ul>
                        </div>
                    </li>
                    <li><a><i class="fa fa-newspaper-o"></i></a>@Html.ActionLink("Tin tức", "tintuc", "Home")</li>
                    <li><a><i class="fa fa-user"></i></a>@Html.ActionLink("Tài khoản", "Dangnhap", "Home")</li>
                    <li><a><i class="fa fa-envelope-o"></i></a>@Html.ActionLink("Liên hệ", "lienhe", "Home")</li>
                </ul>
            </nav>
            <a href=""> <img src="@Url.Content("~/images/favicon.ico")" width="30px" height="30px"></a>
        </div>
    </div>
</div>
