
@{
    ViewBag.Title = "Apple";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="headerA">
    <div class="container">
        <div class="navbar">
            <div class="logo">
                <a href="@Url.Action("Index","Home")"> <img src="@Url.Content("~/images/LGN.png")" width="105px"></a>
            </div>
            <nav>
                <ul>
                    <li>
                        <div class="search-box">
                            <input class="search-txt" type="text" name="" placeholder="Bạn muốn tìm gì?">
                            <a class="search-btn" href="">
                                <i class="fa fa-search" aria-hidden="true"></i>
                            </a>
                        </div>
                    </li>
                    <li><i class="fa fa-home"></i>@Html.ActionLink("Trang chủ", "Index", "Home")</li>
                    <li>
                        <a><i class="fa fa-apple"></i></a>@Html.ActionLink("Apple", "Apple", "Home")
